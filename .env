# dataset_console_light/.env

# Auth выключен
DISABLE_AUTH=1

# Exec limits
TIMEOUT_SECONDS=45
STARTUP_OVERHEAD_SEC=20
MAX_CONCURRENT_RUNS=5
MAX_SCRIPT_BYTES=64000
STDOUT_MAX=200000
STDERR_MAX=200000
MAX_DATASETS=10

# rlimits
RLIMIT_CPU_SECS=5
RLIMIT_AS_BYTES=536870912
RLIMIT_FSIZE=10485760
RLIMIT_NPROC=256

# ClickHouse read-only user
CONSOLE_CH_HOST=192.168.15.39
CONSOLE_CH_PORT=8123
CONSOLE_CH_USER=console_user
CONSOLE_CH_PASSWORD=console123
CONSOLE_CH_DATABASE=default
CONSOLE_CH_SECURE=0

# PostgreSQL
DATABASE_URL=postgresql+psycopg://myuser:admin33@192.168.15.39:5432/mydatabase
META_DATABASE_URL=${DATABASE_URL}
PG_POOL_SIZE=5
PG_MAX_OVERFLOW=5
PG_POOL_PRE_PING=1



# MINIO
MINIO_ENDPOINT=192.168.15.248:9000
MINIO_ACCESS_KEY=minio
MINIO_SECRET_KEY=minio123
MINIO_SECURE=0
PERSIST_BUCKET=dataset-console
PERSIST_PREFIX=runs
AUTO_UPLOAD_MINIO=1

OPENBLAS_NUM_THREADS=1
OMP_NUM_THREADS=1
MKL_NUM_THREADS=1
NUMEXPR_NUM_THREADS=1
MALLOC_CONF=background_thread:false

# DOCKER
SANDBOX_IMAGE=dataset-console:py311-ch_v4
DATAFRAME_KIND=pandas